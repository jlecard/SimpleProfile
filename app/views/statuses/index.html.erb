<h1>Listing statuses</h1>

<table>
  <tr>
    <th>Message</th>
    <th>User</th>
    <th>When</th>
  </tr>

<% @statuses.each do |status| %>
  <tr>
    <td><%= status.message %></td>
    <td><%= status.profile.user.full_name %></td>
    <td><%= status.created_at %></td>
  </tr>
<% end %>
</table>

<br />

